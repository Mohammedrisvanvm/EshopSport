        <hr />
        <section>
          <div class="text-center container">
            <div class="row">
              <div class="row">
                <% jerseyinfo.forEach(i => { %>
                <div class="col-lg-3 col-md-6 mb-4">
                  <div class="card">
                    <div
                      class="bg-image hover-zoom ripple ripple-surface ripple-surface-light"
                      style="height: 300px"
                      data-mdb-ripple-color="light"
                    >
                      <img
                        src="/image/productImage/<%-i.mainImage[0].filename %>"
                        class="w-100 h-100"
                      />
                      <a href="/productPage/<%-i._id %>">
                        <div class="hover-overlay">
                          <div
                            class="mask"
                            style="background-color: rgba(251, 251, 251, 0.15)"
                          ></div>
                        </div>
                      </a>
                    </div>
                    <div class="card-body text-center">
                      <a href="" class="text-reset">
                        <h5 class="card-title mb-2"><%-i.productName %></h5>
                      </a>
                      <a href="" class="text-reset">
                        <p><%-i.category %></p>
                      </a>
                      <h6 class="mb-3 price ">
                        <s class="text-danger ">Rs.<%-i.MRP %></s
                        ><strong class="ms-2 sale text-dark">Rs.<%-i.price %></strong>
                      </h6>
                      <div class="d-flex">
                        <button
                          type="button"
                          class="btn btn-danger mx-4"
                          data-mdb-toggle="tooltip"
                          title="Move to wishlist"
                          onclick="addtowishlist(`<%-i._id %>`)"
