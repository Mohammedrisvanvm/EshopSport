                          <i class="fas fa-heart"></i>
                        </button>
                        <a href="/cart">
                          <button
                            type="button"
                            class="btn btn-primary mx-3"
                            title="buy now"
                            data-mdb-toggle="tooltip"
                            onclick="addtocart(`<%-i._id %>`,`b`)"
